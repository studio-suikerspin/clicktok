{
	"format": "custom",
	"id": "case",
	"label": "Case",
	"repeatable": true,
	"status": true,
	"json": {
		"Main": {
			"uid": {
				"type": "UID",
				"config": {
					"label": "UID"
				}
			},
			"brand_logo": {
				"type": "Image",
				"config": {
					"label": "Brand logo",
					"constraint": {},
					"thumbnails": []
				}
			},
			"title": {
				"type": "Text",
				"config": {
					"label": "Title",
					"placeholder": "Case title"
				}
			},
			"summary": {
				"type": "Text",
				"config": {
					"label": "Summary",
					"placeholder": "Case summary, this should be about 2, maybe 3 sentences"
				}
			},
			"featured_image": {
				"type": "Image",
				"config": {
					"label": "Featured image",
					"constraint": {},
					"thumbnails": []
				}
			},
			"description": {
				"type": "StructuredText",
				"config": {
					"label": "Description",
					"placeholder": "",
					"allowTargetBlank": true,
					"multi": "paragraph,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,embed,list-item,o-list-item"
				}
			},
			"images": {
				"type": "Group",
				"config": {
					"label": "Images",
					"repeat": true,
					"fields": {
						"image": {
							"type": "Image",
							"config": {
								"label": "Image",
								"constraint": {},
								"thumbnails": []
							}
						}
					}
				}
			},
			"videos": {
				"type": "Link",
				"config": {
					"label": "Video URLs",
					"placeholder": "",
					"select": null,
					"allowTargetBlank": false,
					"allowText": false,
					"repeat": true,
					"variants": ["featured", "normal"]
				}
			}
		}
	}
}
